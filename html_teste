  <button onclick="executar()">Executar Código</button>

  <script>
    function executar() {
      // Seu código vai aqui dentro
      let conta = 50;
      conta += Number(prompt("Qual valor deseja inserir ou retirar da conta?"));
      alert(`Atualmente, você possui R$ ${conta} em sua conta`);
    }
  </script>
